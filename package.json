{
  "name": "n8n-nodes-pdf2pic",
  "version": "0.3.0",
  "description": "Node for using pdf2pic to tranform PDFs to images.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jdubdev/n8n-nodes-pdf2pic.git",
  "author": {
    "name": "Jemini Willis",
    "email": "jdubdev@pm.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdubdev/n8n-nodes-pdf2pic.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "build": "rm -rf dist && tsc && gulp build:icons && npm run copyIcon",
    "publishLocal": "npm run build && npm link",
    "debugging": "cd ~/.n8n/nodes && npm link n8n-nodes-pdf2pic && n8n start",
    "copyIcon": "cp -R icon.png dist/nodes/PdfToPic/",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/PdfToPic/PdfToPic.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "eslint": "^8.40.0",
    "gm": "^1.25.0",
    "pdf-to-png-converter": "^3.1.0"
  }
}
